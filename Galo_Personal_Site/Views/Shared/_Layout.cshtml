<!DOCTYPE html>
<html>
<head>
    <meta charset="utf-8">
    <meta http-equiv="X-UA-Compatible" content="IE=edge">
    <meta name="viewport" content="width=device-width, initial-scale=1">
    <meta name="description" content="">
    <meta name="author" content="">
    <title>Galo Lopez</title>
    <link rel="shortcut icon" href="~/img/g3.png" />
    <!-- Bootstrap Core CSS -->
    <link href="~/css/bootstrap.min.css" rel="stylesheet" />
    <!-- Custom Fonts -->
    <link href='https://fonts.googleapis.com/css?family=Open+Sans:300italic,400italic,600italic,700italic,800italic,400,300,600,700,800' rel='stylesheet' type='text/css'>
    <link href='https://fonts.googleapis.com/css?family=Merriweather:400,300,300italic,400italic,700,700italic,900,900italic' rel='stylesheet' type='text/css'>
    <link rel="stylesheet" href="~/font-awesome/css/font-awesome.min.css" type="text/css">
    <link rel="stylesheet" href="~/Content/Site.css" type="text/css">
    <!-- Plugin CSS -->
    <link rel="stylesheet" href="~/css/animate.min.css" type="text/css">
    <!-- Custom CSS -->
    <link rel="stylesheet" href="~/css/creative.css" type="text/css">
    <link href="~/Content/shCoreDefault.css" rel="stylesheet" />
    <link href="~/Content/PagedList.css" rel="stylesheet" />
    <link href="~/css/sequence-theme.basic.min.css" rel="stylesheet" />
</head>
<body id="page-top">
    <nav id="mainNav" class="navbar navbar-default navbar-fixed-top">
        <div class="container-fluid">
            <!-- Brand and toggle get grouped for better mobile display -->
            <div class="navbar-header page-scroll">
                <button type="button" class="navbar-toggle" data-toggle="collapse" data-target="#bs-example-navbar-collapse-1">
                    <span class="sr-only">Toggle navigation</span>
                    <span class="icon-bar"></span>
                    <span class="icon-bar"></span>
                    <span class="icon-bar"></span>
                </button>
                <a class="navbar-brand page-scroll" href="@Url.Action("Index", "Home")">Galo Lopez</a>
                @Html.Partial("_LoginPartial")
            </div>
            <!-- Collect the nav links, forms, and other content for toggling -->
            <div class="collapse navbar-collapse" id="bs-example-navbar-collapse-1">
                <ul class="nav navbar-nav navbar-right">
                    <li><a href="@Url.Action("About", "Home")">About Me</a></li>
                    <li><a href="@Url.Action("Profile", "Home")">Profile</a></li>
                    <li><a href="@Url.Action("Portfolio", "Home")">Portfolio</a></li>
                    <li><a href="@Url.Action("Index", "BlogPosts")">Blog</a></li>
                    <li><a href="@Url.Action("Contact", "Home")">Contact</a></li>
                </ul>
            </div>

            <!-- /.navbar-collapse -->
        </div>
        <!-- /.container-fluid -->
    </nav>

        @RenderBody()
        @*<hr />
        <footer>
            <p>&copy; @DateTime.Now.Year - My ASP.NET Application</p>
        </footer>*@

    <!-- jQuery -->
    <!-- Bootstrap Core JavaScript -->
    <script src="~/Scripts/jquery-2.1.4.min.js"></script>
    <script src="~/js/bootstrap.min.js"></script>
    <!-- Plugin JavaScript -->
    <script src="~/js/jquery.easing.min.js"></script>
    <script src="~/js/jquery.fittext.js"></script>
    <script src="~/js/wow.min.js"></script>
    <!-- Custom Theme JavaScript -->
    <script src="~/js/creative.js"></script>
    @*<script src="~/Scripts/pdf.js"></script>*@
    <script src="~/Scripts/shCore.js"></script>
    <script src="~/Scripts/shBrushJScript.js"></script>
    <script src="~/ckeditor/ckeditor.js"></script>
    <script>SyntaxHighlighter.all();</script>
    <script src="~/js/jquery.dotdotdot.min.js"></script>
    <script src="~/js/jquery.dotdotdot.js"></script>
    <script src="~/js/hammer.min.js"></script>
    <script src="~/js/sequence-theme.basic.min.js"></script>
    <script src="~/js/sequence.min.js"></script>

    @RenderSection("scripts", required: false)
</body>
</html>
